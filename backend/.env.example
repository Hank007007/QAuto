# ==================== LLM Proxy Config ====================
USE_PROXY=True
LLM_TYPE=gpt-4o
API_KEY=your-api-key-here

# ==================== 通用配置 ====================
# 选择使用的模型：chatgpt / gemini
USE_MODEL=chatgpt



